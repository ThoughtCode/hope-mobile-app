{
  "expo": {
    "name": "Noc Noc",
    "slug": "noc-noc",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "version": "2.1.5",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/img/logo_azul_no_transparente.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nocnoc.app"
    },
    "locales": {
      "es": "./languages/es.json"
    },
    "android": {
      "versionCode": 15,
      "package": "com.nocnoc.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "INTERNET"
      ]
    },
    "notification": {
      "icon": "./assets/img/notification_logo.png"
    }
  }
}